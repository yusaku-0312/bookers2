<h1>User Info</h1>
<p><%= image_tag @user.get_profile_image(100, 100), class:"mt-3 rounded-circle"; %></p>
<p><strong>Name</strong><%= @user.name%></p>
<p><storng>Introduction</storng><%= @user.introduction%></p>
<hr>
<h1>Users</h1>
<% @users.each do |user| %>
  <p><%= image_tag user.get_profile_image(100, 100), class:"mt-3 rounded-circle"; %></p>
  <p><strong>Name</strong><%= user.name%></p>
  <p><storng>Introduction</storng><%= user.introduction%></p>
<% end %>